{
   "name":"GeoTag-X Chrome Sourcerer",
   "description":"Send images to GeoTag-X while you browse",
   "version":"0.8",
   "permissions":[
      "contextMenus",
      "tabs"
   ],
   "icons":{
      "16":"icons/icon16.png",
      "48":"icons/icon48.png",
      "128":"icons/icon128.png"
   },
   "background":{
      "scripts":[
         "lib/base64.min.js",
         "lib/jquery-2.1.4.min.js",
         "geotagx-chrome-sourcerer.js"
      ]
   },
   "browser_action":{
      "default_icon":{
         "16":"icons/icon16.png",
         "48":"icons/icon48.png"
      },
      "default_title":"GeoTag-X",
      "default_popup":"popup.html"
   },
   "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self'",
   "manifest_version":2
}
